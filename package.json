{
  "name": "pomodoro",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:jsx": "eslint **/*.jsx",
    "lint:jsx:fix": "eslint **/*.jsx --fix",
    "stylelint": "stylelint **/*.{css,scss,sass,less}",
    "stylelint:fix": "stylelint **/*.{css,scss,sass,less} --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:jsx": "prettier --write \"**/*.jsx\"",
    "format:css": "prettier --write \"**/*.{css,scss,sass,less}\"",
    "quality": "npm run lint && npm run format:check && npm run test",
    "quality:jsx": "npm run lint:jsx && npm run format:jsx && npm run stylelint",
    "precommit": "npm run quality",
    "build": "expo export:web"
  },
  "dependencies": {
    "expo": "~53.0.10",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "expo-av": "~15.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.4",
    "jest": "^29.7.0",
    "license-checker": "^25.0.1",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0"
  },
  "private": true
}
